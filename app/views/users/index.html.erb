<p style="color: green"><%= notice %></p>
<h1>Listting Users</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <%= @users.each do | user | %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= link_to 'Show',user %></td>
      <td><%= link_to 'Edit',edit_user_path(user) %></td>
      <td><%= link_to  'Destroy',user,mthod:delete,
      data: {
        comfirm:'Are you sure??'
      } %></td>
    </tr>
  </table>
  <div id="users">
    <% @users.each do |user| %>
      <%= render user %>
      <p>
        <%= link_to "Show this user", user %>
      </p>
    <% end %>
  </div>
  <%= link_to "New user", new_user_path %>
