<p style="color: green"><%= notice %></p>
<h1>Listting Users</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <% @users.each do |user| %>
    <tr>
      <td class="table-td"><%= user.name %></td>
      <td class="table-td"><%= user.email %></td>
      <td class="table-td"><%= link_to 'Show',user %></td>
      <td class="table-td"><%= link_to 'Edit',edit_user_path(user) %></td>
      <td class="table-td">
        <%= link_to 'Destroy',user,mthod: :delete,
          data: {
            comfirm:'Are you sure??'
          }
        %>
      </td>
    </tr>
  <% end %>
</table>
<br>
<%= link_to "New user", new_user_path %>
